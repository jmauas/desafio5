<div class="container m-5">
  <h1>TABLA DE PRODUCTOS REGISTRADOS</h1>
  <table class="table table-primary table-striped">
    <thead>
        <tr>
        <th scope="col">Nombre Producto</th>
        <th scope="col">Precio</th>
        <th scope="col">Imagen</th>
        </tr>
    </thead>
    <tbody>
        <% productos.forEach(function(prod){ %>
          <tr>
              <td class="nombreProd"><%= prod.title %></td>
              <td class="prProd"><%= prod.price %></td>
              <td><img alt="Foto" class="fotoProd" src=<%= prod.thumbnail %>></td>
          </tr>
          <% }) %>
    </tbody>
  </table>
  <a href="/" class="btn btn-lg btn-info" role="button"><i class="bi-house-fill"></i> Inicio</a>
</div>